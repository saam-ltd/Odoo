<?xml version="1.0" encoding="utf-8" ?>
<odoo>
    <!-- Hide Contacts -->
    <record model="ir.module.category" id="module_contacts">
        <field name="name">Hide Contacts</field>
        <field name="sequence">4</field>
    </record>

    <record id="group_hide_contacts" model="res.groups">
        <field name="name">Hide Contacts</field>
        <field name="category_id" ref="module_contacts"/>
    </record>

    <!-- Hide Accounting -> Vendors -->
    <record model="ir.module.category" id="module_account_vendors">
        <field name="name">Hide Vendors</field>
        <field name="sequence">5</field>
    </record>

    <record id="group_account_vendors" model="res.groups">
        <field name="name">Hide Accounting -> Vendors</field>
        <field name="category_id" ref="module_account_vendors"/>
    </record>

</odoo>
